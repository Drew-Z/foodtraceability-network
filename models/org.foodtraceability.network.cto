/**
 * 初步构想
 */

namespace org.foodtraceability.network

asset FarmProduct identified by productId {
  o String productId  //每次生产自动产生
  o ProductType type  //农产品种类
  o String picHash    //图片hash值
  o DateTime time     //生产时间
  o String producer   //生产者
  -->Trader owner
}

abstract participant User identified by userId {
  o String userId
  o String name
  o String tel
}

participant Regulator extends User{
}

participant Trader extends User {
}

transaction Trade {
  -->FarmProduct farmProduct
  -->Trader newOwner
}

enum ProductType {
  o APPLE
  o ORANGE
}

event TradeNotification {
  -->FarmProduct farmProduct
}
